msgid "Please wait, this can take some time"
msgstr "Itxaron mesedez, baliteke honek denbora pixka bat behar izatea"

#: usr/lib/linuxmint/mintLocale/install_remove.py:197
#: usr/lib/linuxmint/mintLocale/mintLocale.py:473
msgid "Installation is complete"
msgstr "Instalazioa amaitu da"

#: usr/lib/linuxmint/mintLocale/install_remove.py:236
