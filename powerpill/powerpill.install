post_upgrade()
{
  oldver=$2
  if [[ $(vercmp $oldver 2012.12.12.3) -le 0 ]]
  then
    echo '==> WARNING: The following changes have been made to powerpill.json:
             * rsync/server -> rsync/servers: this is now a list of servers
             * rsync/db only: if true, Rsync will only be used for DB downloads'
  fi
}
